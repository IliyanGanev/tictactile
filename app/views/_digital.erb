<div class="section" id="digital">
<div id="box4" style="margin-top: -0.5em;">
	<div class="wrapper">
		<h2 style="font-size: 20px; padding-top: 4em; padding-bottom: 3em; font-family: myFirstFont;">digital</h2>


			<% @digital.each do |project| %>	
			<div class='icon'>
				<img class="<%= if project.include?(:override_thumbnail_class) then project[:override_thumbnail_class] else "thumb01" end %>" style="cursor:pointer" src="<%= project[:thumbnail] %>"
				onclick="onThumbnailClick('digital', '<%= project[:item_anchor] %>');"  class="w3-hover-opacity">
			</div><div id="<%= project[:item_anchor] %>" class="w3-modal" onclick="onModelCancelClick(this);">
				<span class="w3-button w3-hover-red w3-xlarge w3-display-topright">&times;</span>
				<div style="background:rgba(0, 0, 0, 0.0);" class="w3-modal-content w3-animate-zoom">
					<img class="<%= if project.include?(:override_image_class) then project[:override_image_class] else "img01" end %>" src="<%= project[:image] %>">
					<p style="font-family: myFirstFont; color: grey; background:rgba(0, 0, 0, 0.0); margin-top: 0px; font-size: 13px;"><mark class="mark"><%= project[:title] %></mark></p>
				</div>
			</div>
			<%end%>


	</div>
</div>
</div>

